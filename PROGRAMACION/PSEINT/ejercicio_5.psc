Proceso ejercicio_5
	Definir horas, sueldo_hora como reales;
	Definir salario como real;
	
	Escribir "¿Cuántas horas has trabajado?";
	Leer horas;
	
	Escribir "¿Cuál es el sueldo por hora?";
	Leer sueldo_hora;
	
	salario <- horas * sueldo_hora;
	
	Escribir "Su salario es de: ", salario;
	
	
FinProceso
